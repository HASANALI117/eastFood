<h2 style="margin: 30px auto">Profile</h2>

<table class="table table-striped alignmiddle" style="text-align: center">
  <tr class="table-secondary">
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email Address</th>
  </tr>
  <tr class="table-light">
    <td class="td-profile"><%= currentUser.firstName %></td>
    <td class="td-profile"><%= currentUser.lastName %></td>
    <td class="td-profile"><%= currentUser.emailAddress %></td>
    <td>
      <a href="/user/edit?id=<%= currentUser._id %>">Edit</a>
    </td>
  </tr>
</table>
<a href="/auth/logout" class="btn btn-dark" style="margin: 10px 30px"
  >log Out</a
>
